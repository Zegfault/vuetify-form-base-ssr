{
    "name": "vuetify-form-base-ssr",
    "version": "1.0.3",
    "description": "Vuetify form generator",
    "main": "dist/vuetify-form-base-ssr.ssr.js",
    "browser": "dist/vuetify-form-base-ssr.esm.js",
    "module": "dist/vuetify-form-base-ssr.esm.js",
    "unpkg": "dist/vuetify-form-base-ssr.min.js",
    "files": [
        "dist/*",
        "src/**/*.vue"
    ],
    "sideEffects": false,
    "scripts": {
        "serve": "vue-cli-service serve dev/serve.js",
        "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
    },
    "dependencies": {
        "v-mask": "^2.2.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pooyagolchian/vuetify-form-base-ssr"
    },
    "keywords": [
        "vue",
        "vue 2.0",
        "vuetify",
        "form",
        "generator",
        "json",
        "schema",
        "model",
        "date",
        "responsive",
        "grid",
        "reactive",
        "select",
        "multiselect",
        "grid",
        "controls",
        "Nuxt.js form generator base on schema",
        "Vue.js form generator base on schema"
    ],
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@rollup/plugin-alias": "^3.1.1",
        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.1",
        "@rollup/plugin-replace": "^2.3.4",
        "@vue/cli-plugin-babel": "^4.5.10",
        "@vue/cli-service": "^4.5.10",
        "cross-env": "^7.0.3",
        "minimist": "^1.2.5",
        "rollup": "^2.36.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-vue": "^5.1.9",
        "vue": "^2.6.12",
        "vue-template-compiler": "^2.6.12"
    },
    "peerDependencies": {
        "vue": "^2.6.12"
    },
    "license": "MIT",
    "engines": {
        "node": ">=12"
    },
    "bugs": {
        "url": "https://github.com/pooyagolchian/vuetify-form-base-ssr/issues"
    },
    "homepage": "https://github.com/pooyagolchian/vuetify-form-base-ssr#readme"
}
